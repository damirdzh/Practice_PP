<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EAE4CC"
    tools:context=".svinyshka6">

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="434dp"
        android:layout_height="235dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/paxillusinvolutus" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="399dp"
        android:layout_height="353dp"
        android:text="Мякоть желтоватого цвета, но на воздухе становится бурой, а при варке чернеет. Похожие грибы: Своим видом напоминает млечники, но отличается от них отсутствием млечного сока. Съедобность: Ядовитый гриб! В сыром виде свинушка тонкая может быть смертельно ядовитой."
        android:textSize="26dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView17"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button25"
        android:layout_width="149dp"
        android:layout_height="64dp"
        android:layout_marginStart="5dp"
        android:background="#00FFFFFF"
        android:onClick="button25"
        android:text="В главное меню"
        android:textColor="#000000"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.96"
        tools:ignore="TextSizeCheck" />

    <Button
        android:id="@+id/button26"
        android:layout_width="149dp"
        android:layout_height="64dp"
        android:layout_marginEnd="5dp"
        android:background="#00FFFFFF"
        android:onClick="button26"
        android:text="Назад"
        android:textColor="#000000"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.917"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.959"
        tools:ignore="TextSizeCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>